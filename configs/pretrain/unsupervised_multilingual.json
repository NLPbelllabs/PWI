{   
    // batch size & optimizer
    "batch_size": 32, //144
    "valid_batch_size": 32,
    "optim": "bert",
    "lr": 6e-5,
    "epochs": 100,
    "tqdm": true,
    "max_seq_length": 30,
    "report_every": 500,
    "warmup_ratio": 0.02,
    "num_workers": 0, //32,
    "val_num_workers": 0, //2,
    "partial_dataset": 1,
