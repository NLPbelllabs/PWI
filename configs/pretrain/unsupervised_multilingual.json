    "inbatch_random": true,
    "disable_random_feat": false,
    "custom_collact_fn": true,
    "faster_loading": true,
    
    "compress_memory": true,
    "random_seed": 42,

    "on_memory": false,

    "train": null, // specify here if we want aligned data
